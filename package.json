{
  "name": "childnode-remove",
  "version": "1.0.0",
  "description": "ChildNode.remove() polyfill for IE",
  "main": "src/index.js",
  "scripts": {
    "compile": "browserify src/index.js -o dist/childnode-remove.js --standalone ChildNodeRemove",
    "compress": "uglifyjs dist/childnode-remove.js --compress --mangle > dist/childnode-remove.min.js",
    "build": "npm run compile && npm run compress"
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/JapMul/childnode-remove"
  },
  "keywords": [
      "ChildNode",
      "remove",
      "polyfill"
  ],
  "author": "Jap Mul <jap@branchonline.nl>",
  "license": "MIT",
  "bugs": {
      "url": "https://github.com/JapMul/childnode-remove/issues"
  },
  "devDependencies": {
      "browserify": "^10.1.3",
      "uglify-js": "^2.4.21"
  }
}
